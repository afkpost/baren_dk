<div class='events'>
    <h3>{{title}}</h3>
    <ul data-role='listview' class='ui-mini'>
        {{#events}}
            <li data-role='list-divider' class='list-header'>{{month}}</li>
            {{#events}}
                <li data-role='collapsible data-isset='false' data-icon='false'>
                    <h3>{{title}}</h3>
                    <h4>{{date}}</h4>
                </li>
            {{/events}}
        {{/events}}
    </ul>
</div>